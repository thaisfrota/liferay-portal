	macro assertDataInSelectFromListField {
		if (!(isSet(index))) {
			var index = "1";
		}

		AssertTextEquals(
			index = "${index}",
			key_fieldLabel = "${fieldLabel}",
			locator1 = "DataEngineRenderer#SELECT_FROM_LIST_SELECTED_OPTION_INDEXED",
			value1 = "${optionValue}");
	}

